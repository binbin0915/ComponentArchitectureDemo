<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wangkai.compose">

    <application
        android:name="com.library.base.application.DebugApplication"
        android:allowBackup="false"
        android:icon="@mipmap/compose_ic_launcher"
        android:label="@string/compose_app_name"
        android:roundIcon="@mipmap/compose_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Compose_Theme.MyApplication">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>